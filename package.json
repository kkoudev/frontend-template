{
  "name": "frontend-template",
  "version": "0.8.0",
  "description": "Template of frontend",
  "author": "kkoudev (Koichi Nagaoka)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kkoudev/frontend-template.git"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "run-s clean && run-p watch:* server",
    "clean": "node ./node_scripts/tasks/clean.js",
    "build:material": "node ./node_scripts/tasks/material.js",
    "build:postcss": "node ./node_scripts/tasks/postcss.js",
    "build:image": "node ./node_scripts/tasks/image.js",
    "build:pug": "node ./node_scripts/tasks/pug.js",
    "build:eslint": "node ./node_scripts/tasks/eslint.js",
    "build:fusebox": "node ./node_scripts/tasks/fusebox.js",
    "build": "run-p build:*",
    "watch:material": "cross-env NODE_WATCH=true nodemon --watch ./node_scripts --watch ./config ./node_scripts/tasks/material.js",
    "watch:postcss": "cross-env NODE_WATCH=true STYLE_GUIDE=true nodemon --watch ./node_scripts --watch ./config ./node_scripts/tasks/postcss.js",
    "watch:image": "cross-env NODE_WATCH=true nodemon --watch ./node_scripts --watch ./config ./node_scripts/tasks/image.js",
    "watch:pug": "cross-env NODE_WATCH=true nodemon --watch ./node_scripts --watch ./config ./node_scripts/tasks/pug.js",
    "watch:eslint": "cross-env NODE_WATCH=true nodemon --watch ./node_scripts --watch ./config ./node_scripts/tasks/eslint.js",
    "watch:fusebox": "cross-env NODE_WATCH=true nodemon --watch ./node_scripts --watch ./config ./node_scripts/tasks/fusebox.js",
    "server:backend": "nodemon --watch ./server --watch ./config ./server/app.js",
    "server:frontend": "nodemon --watch ./node_scripts --watch ./config ./node_scripts/tasks/server.js",
    "server": "run-p server:*",
    "dist": "cross-env NODE_ENV=production COMPRESS_IMAGE=true run-s clean build",
    "guide": "cross-env STYLE_GUIDE=true node ./node_scripts/tasks/postcss.js"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "dependencies": {
    "autoprefixer": "^9.1.3",
    "avoriaz": "^6.3.0",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "blazy": "^1.8.2",
    "browser-sync": "^2.24.7",
    "chalk": "^2.4.1",
    "chokidar": "^2.0.4",
    "cluster": "^0.7.7",
    "compress-images": "^1.4.0",
    "consolidate": "^0.15.1",
    "cross-env": "^5.2.0",
    "css-mqpacker": "^7.0.0",
    "csswring": "^7.0.0",
    "eslint": "^5.5.0",
    "eslint-watch": "^4.0.2",
    "express": "^4.16.3",
    "fs-extra": "^7.0.0",
    "fuse-box": "^3.5.0",
    "glob": "^7.1.3",
    "jest": "^23.5.0",
    "lodash.assign": "^4.2.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.0",
    "node-notifier": "^5.2.1",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "postcss": "^7.0.2",
    "postcss-assets": "^5.0.0",
    "postcss-calc": "^6.0.1",
    "postcss-easy-import": "^3.0.0",
    "postcss-functions": "^3.0.0",
    "postcss-hexrgba": "^1.0.1",
    "postcss-mixins": "^6.2.0",
    "postcss-nested-props": "^2.0.0",
    "postcss-reporter": "^6.0.0",
    "postcss-selector-parser": "^5.0.0-rc.3",
    "postcss-sorting": "^4.0.0",
    "postcss-sprites": "^4.2.1",
    "postcss-style-guide": "^0.14.0",
    "postcss-webfont": "^1.1.0",
    "precss": "^3.1.2",
    "pug": "^2.0.3",
    "rxjs": "^6.3.1",
    "sanitize.css": "^7.0.1",
    "stylelint": "^9.5.0",
    "stylelint-config-sugarss-recommended": "^1.1.0",
    "sugarss": "^2.0.0",
    "typescript": "^3.0.3",
    "unquote": "^1.1.1",
    "vue": "^2.5.17",
    "vue-hot-reload-api": "^2.3.0",
    "vue-server-renderer": "^2.5.17",
    "vue-template-compiler": "^2.5.17",
    "vue-template-es2015-compiler": "^1.6.0",
    "vuex": "^3.0.1",
    "webfontloader": "^1.6.28"
  }
}
